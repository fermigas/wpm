#!/usr/bin/env bash

BINTYP="default"
BINURL="http://youtube-dl.org/downloads/2013.05.23/youtube-dl.exe"
MD5="c52e32387770395629b96d22cede833f"
O_FILE="youtube-dl.exe"
#declare -a DEPEND=( "python27" )

function setup {
    echo "cp $O_FILE ~/bin/wpm/bin/$O_FILE"
}

function check_install {
    which youtube-dl >> stderr
    echo $?
}

case "$1" in 
    "bintype" )
        echo $BINTYP;;
    "binurl" )
        echo $BINURL;;
    "depend" )
        echo ${DEPEND[@]};;
    "o_file" ) 
        echo $O_FILE ;; 
    "setup_command" )
        setup;;
    "check_install" ) 
        check_install;;
    "author" ) 
        echo "@hugo_dc" ;; 
esac

