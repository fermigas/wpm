#!/usr/bin/env bash

BINTYP="default"
BINURL="https://fleex.blob.core.windows.net/misc/installer/fleex-1.1.0.exe"
O_FILE="fleex-1.1.0.exe"
MD5="a"
SETUP="cmd //c fleex-1.1.0.exe"

function check_install {
    a=$(which fleex) 
    echo $?
}

case "$1" in 
    "bintype" )
        echo $BINTYP;;
    "binurl" )
        echo $BINURL;;
    "o_file" )
        echo $O_FILE;;
    "md5" ) 
        echo $MD5;;
    "setup_command" )
        echo $SETUP ;;
esac

