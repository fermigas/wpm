#!/bin/bash 

BINTYP="msi"
BINURL="http://python.org/ftp/python/2.7.5/python-2.7.5.msi"
O_FILE="python-2.7.5.msi"
MD5=""
declare -a DEPEND=("msiexec")

function check_install {
    type python  >> stderr 
    echo $?
}

case "$1" in 
    "bintype" )
        echo $BINTYP;;
    "binurl" )
        echo $BINURL;;
    "o_file" )
        echo $O_FILE;;
    "md5" ) 
        echo $MD5;;
    "exetype" )
        echo $EXETYP;;
    "depend" )
        echo ${DEPEND[@]};;
    "check_install" )
        check_install;;

esac
