#!/usr/bin/env bash

BINTYP="default"
BINURL="http://python.org/ftp/python/2.7.5/python-2.7.5.msi"
O_FILE="python-2.7.5.msi"
MD5="0006d6219160ce6abe711a71c835ebb0"
declare -a DEPEND=("msiexec")

SETUP="msiexec -i $O_FILE"

function check_install {
    which python  >> ~/.bin/wpm/log 
    echo $?
}

case "$1" in 
    "bintype" )
        echo $BINTYP;;
    "binurl" )
        echo $BINURL;;
    "o_file" )
        echo $O_FILE;;
    "md5" ) 
        echo $MD5;;
    "depend" )
        echo ${DEPEND[@]};;
    "check_install" )
        check_install;;
    "setup_command" )
        echo $SETUP ;;
esac
