#!/bin/bash

BINTYP="default"
BINURL="http://nodejs.org/dist/v0.10.8/node-v0.10.8-x86.msi"
O_FILE="node-v0.10.8-x86.msi"
EXETYP="msiexec -i "
MD5="a363dbcaf9f2fbee955d890351afba34"

case "$1" in 
    "bintype" )
        echo $BINTYP;;
    "binurl" )
        echo $BINURL;;
    "o_file" )
        echo $O_FILE;;
    "md5" ) 
        echo $MD5;;
    "exetype" )
        echo $EXETYP;;
esac

